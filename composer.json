{
  "name": "omaximus/laravel-advanced-seeder",
  "description": "Advanced seeding for laravel framework",
  "keywords": ["laravel", "seed"],
  "require": {
    "php": ">=5.6.4",
    "illuminate/console": "^5.0,<5.6",
    "illuminate/database": "^5.0,<5.6",
    "illuminate/filesystem": "^5.0,<5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.4.0",
    "mockery/mockery": "^0.9.5"
  },
  "autoload": {
    "psr-4": {
      "Pisochek\\LaravelSeeder\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pisochek\\LaravelSeeder\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pisochek\\LaravelSeeder\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
